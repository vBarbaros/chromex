{
  "manifest_version": 3,
  "name": "Trade-Off Assessment Tool",
  "version": "1.0",
  "description": "You can quickly verify if something is worth it relative to your golden standard",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "assets/images/worthit-32x32.png",
      "48": "assets/images/worthit-132x132.png",
      "128": "assets/images/worthit-132x132.png"
    }
  },
  "icons": {
    "16": "assets/images/worthit-32x32.png",
    "48": "assets/images/worthit-132x132.png",
    "128": "assets/images/worthit-132x132.png"
  }
}
